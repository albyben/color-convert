{
  "name": "color-convert-ie",
  "description": "Plain color conversion functions",
  "version": "2.0.2",
  "author": "Josh Junon <josh@junon.me>",
  "contributors": [
    "Heather Arthur <fayearthur@gmail.com>"
  ],
  "license": "MIT",
  "repository": "Qix-/color-convert",
  "main": "./lib/index.js",
  "module": "./lib/index.js",
  "scripts": {
    "pretest": "xo",
    "test": "node test/basic.js",
    "build:allInOne": "spack",
    "build": "swc ./index.js ./conversions.js ./route.js -d lib"
  },
  "engines": {
    "node": ">=7.0.0"
  },
  "keywords": [
    "color",
    "colour",
    "convert",
    "converter",
    "conversion",
    "rgb",
    "hsl",
    "hsv",
    "hwb",
    "cmyk",
    "ansi",
    "ansi16"
  ],
  "files": [
    "lib"
  ],
  "xo": {
    "rules": {
      "default-case": 0,
      "no-inline-comments": 0,
      "operator-linebreak": 0,
      "unicorn/prefer-exponentiation-operator": 0
    }
  },
  "devDependencies": {
    "@swc/cli": "^0.3.10",
    "@swc/core": "^1.4.2",
    "chalk": "^2.4.2",
    "jimp": "^0.16.1",
    "xo": "^0.24.0"
  },
  "dependencies": {
    "color-name": "~1.1.4"
  }
}
